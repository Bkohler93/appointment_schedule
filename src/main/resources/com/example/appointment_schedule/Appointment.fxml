                  </HBox>
                  <HBox alignment="CENTER_LEFT">
                     <children>
                        <Label prefWidth="100.0" text="Start" />
                        <TextField fx:id="startDateTextField" prefWidth="95.0" promptText="YYYY/MM/DD">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </TextField>
                        <TextField fx:id="startTimeTextField" prefWidth="80.0" promptText="23:59" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
